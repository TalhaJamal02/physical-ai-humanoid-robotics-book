{
  "version": 2,
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/docs/build/$1" }
  ]
}
